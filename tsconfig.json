{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@app/*": [
        "./app/*"
      ],
      "@/*": [
        "./*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@ui/*": [
        "./components/ui/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@features/*": [
        "./features/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@theme/*": [
        "./theme/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}